{
  "name": "datocms-plugin-trustfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "check:ts": "tsc --noEmit",
    "start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
    "build": "cross-env PUBLIC_URL='.' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "yarn build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "datocms-plugin-sdk": "^0.6.1",
    "datocms-react-ui": "^0.6.4",
    "final-form": "^4.20.7",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.57",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "eslint-config-xo-bizon": "^1.1.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.31.7",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.2",
    "xo": "^0.52.3"
  },
  "xo": {
    "extends": [
      "xo-bizon",
      "xo-react"
    ],
    "semicolon": false,
    "space": 2,
    "prettier": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "@typescript-eslint/triple-slash-reference": "off",
      "n/file-extension-in-import": "off"
    }
  },
  "prettier": {
    "printWidth": 100,
    "jsxSingleQuote": true
  },
  "datoCmsPlugin": {
    "title": "",
    "previewImage": "",
    "coverImage": "",
    "entryPoint": "build/index.html"
  }
}
